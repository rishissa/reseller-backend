{
  "kind": "collectionType",
  "collectionName": "leads",
  "info": {
    "singularName": "lead",
    "pluralName": "leads",
    "displayName": "Lead",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": false
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "country_code": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "NEW",
        "CALLING",
        "CALLED",
        "CONVERTED",
        "COMPLETED"
      ]
    },
    "source": {
      "type": "enumeration",
      "enum": [
        "WHATSAPP",
        "INSTAGRAM",
        "SOCIAL_SELLER_WEBSITE",
        "YOUTUBE_CHANNEL"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "lead_raised"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "assigned_to": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "consumer_note": {
      "type": "text"
    },
    "staff_note": {
      "type": "text"
    }
  }
}
