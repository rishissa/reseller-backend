{
  "kind": "collectionType",
  "collectionName": "plans",
  "info": {
    "singularName": "plan",
    "pluralName": "plans",
    "displayName": "Plan",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "days": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "quantity": {
      "type": "integer",
      "default": 0
    },
    "premiumPricing": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "codAllowed": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "prepaidAllowed": {
      "type": "boolean",
      "default": false
    },
    "isActive": {
      "type": "boolean",
      "default": false
    },
    "subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subscription.subscription",
      "mappedBy": "plan"
    },
    "price": {
      "type": "decimal",
      "default": 0,
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    },
    "plan_metric": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::plan-metric.plan-metric",
      "mappedBy": "plan"
    }
  }
}
