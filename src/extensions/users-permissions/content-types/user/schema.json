{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "isPremium": {
      "type": "boolean",
      "default": false
    },
    "phone": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "isAdmin": {
      "type": "boolean",
      "default": false
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "users_permissions_user"
    },
    "wishList": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "cart_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-product.cart-product",
      "mappedBy": "user"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "user"
    },
    "resetTokenExpiration": {
      "type": "datetime",
      "private": true
    },
    "subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subscription.subscription",
      "mappedBy": "users_permissions_user"
    },
    "wallets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wallet.wallet",
      "mappedBy": "users_permissions_user"
    },
    "wallet_balance": {
      "type": "decimal",
      "default": 0
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "user"
    },
    "countryCode": {
      "type": "string",
      "default": "+91"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "fcmToken": {
      "type": "string"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "users_permissions_user"
    },
    "businessName": {
      "type": "string"
    },
    "businessLogo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "ordersCount": {
      "type": "integer"
    },
    "profit": {
      "type": "decimal"
    },
    "shares": {
      "type": "integer"
    },
    "activity_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity-log.activity-log",
      "mappedBy": "user"
    },
    "lead_raiseds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lead.lead",
      "mappedBy": "user"
    },
    "metric": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-metric.user-metric",
      "inversedBy": "user"
    },
    "cart": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart.cart",
      "mappedBy": "user"
    },
    "otp": {
      "type": "string"
    },
    "otp_expiration": {
      "type": "datetime"
    },
    "personal_id": {
      "type": "string"
    },
    "admin_subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::admin-subscription.admin-subscription",
      "mappedBy": "users_permissions_user"
    }
  }
}
